<div class="col-lg-offset-1 col-lg-10 col-sm-12 col-xs-12 col-md-offset-1 col-md-12">

<p>
  <strong>User:</strong>
  <%= @album.user %>
</p>

<p>
  <strong>Title:</strong>
  <%= @album.title %>
</p>

  <% if user_signed_in? && current_user == @user  %>
      <%= link_to 'Edit', edit_album_path(@album), class: 'btn btn-primary' %> |
  <% end  %>
<%= link_to 'Back', albums_path, class: 'btn btn-default' %>
</div>